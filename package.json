{
  "name": "@veksa/cat.ts",
  "version": "0.0.2",
  "author": "Veksa",
  "description": "meow",
  "keywords": [
    "meow",
    "meow"
  ],
  "license": "MIT",
  "packageManager": "yarn@4.0.2",
  "engines": {
    "node": ">=18.16.0"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "compile": "tsc --noEmit",
    "clean": "shx rm -rf lib && shx rm -rf es",
    "copy-files": "shx cp -R src/resources es/resources && shx cp -R src/resources lib/resources",
    "build:cjs": "tsc -p tsconfig.build.json --outDir lib -d -t es5",
    "build:es": "tsc -p tsconfig.build.json --outDir es -m es6",
    "build": "yarn clean && yarn build:cjs && yarn build:es && yarn copy-files"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/",
    "es/"
  ],
  "sideEffects": false,
  "homepage": "https://github.com/veksa/cat.ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/veksa/cat.ts.git"
  },
  "bugs": {
    "url": "https://github.com/veksa/cat.ts/issues"
  },
  "devDependencies": {
    "@types/animejs": "3.1.12",
    "@types/node": "20.10.5",
    "shx": "0.3.4",
    "tslib": "2.6.2",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "animejs": "3.2.2"
  }
}
